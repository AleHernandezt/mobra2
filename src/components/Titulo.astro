---
interface Props {
  title: string;
  subtitle: string;
}

const { title } = Astro.props;
const { subtitle } = Astro.props;
---

<div class="flex flex-col w-full justify-start gap-8">
  <h2 id="dynamicTitle" class="text-3xl text-yellow-500 font-bold uppercase">{title}</h2>
  <h3 class="text-xl pb-12 sm:text-lg">
    {subtitle}
  </h3>
</div>